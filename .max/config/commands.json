{
  "registry": {
    "auto_discover": true,
    "cache_commands": true,
    "validate_syntax": true
  },
  "execution": {
    "timeout": 30,
    "retry_attempts": 3,
    "parallel_execution": true
  },
  "aliases": {
    "enabled": true,
    "auto_generate": true,
    "conflict_resolution": "warn"
  },
  "help": {
    "auto_generate": true,
    "include_examples": true,
    "format": "markdown"
  }
}